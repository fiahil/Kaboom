<Window x:Class="KaboomEditor.Pages.Checkpoint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Checkpoint" Height="192" Width="230" ResizeMode="NoResize" WindowStyle="None">
    <Grid Margin="0">
        <Slider x:Name="ActivationSlider" HorizontalAlignment="Left" Margin="74.52,36,0,0" VerticalAlignment="Top" Width="90" Maximum="1" SmallChange="1" RenderTransformOrigin="1.36,3.427" ValueChanged="ActivationSlider_ValueChanged"/>
        <Label Content="Activated" HorizontalAlignment="Left" Margin="10,28.04,0,0" VerticalAlignment="Top"/>
        <Label x:Name="StateLabel" Content="False" HorizontalAlignment="Left" Margin="169.52,28.04,0,0" VerticalAlignment="Top"/>
        <Label Content="Bombset" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="BombsetButton" HorizontalAlignment="Left" Height="23" Margin="74.52,76,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="90"/>
        <Button x:Name="OkButton" Content="Ok" HorizontalAlignment="Left" Margin="10,123,0,0" VerticalAlignment="Top" Width="75" Click="OkButton_Click"/>

    </Grid>
</Window>
